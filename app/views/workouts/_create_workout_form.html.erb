
<%= form_with model: @workout do |f|  %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<%= f.label "Exercises" %>
	<%= f.select("exercise_id", Exercise.all.collect {|exercise| [ exercise.name, exercise.id ] }, { include_blank: true }, multiple: true ) %>
	<%= f.submit "Create Workout", class: "btn" %>
<% end %>





<h3>Or create new exercise...</h3>