<%= form_with model: @exercise do |f|  %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<%= f.label "Muscle groups involved" %>
	<%= f.select("muscle_group_id", MuscleGroup.all.collect {|muscle| [ muscle.muscles, muscle.id ] }, { include_blank: true }, multiple: true ) %>
	<%= f.submit "Create Exercise", class: "btn" %>
<% end %>